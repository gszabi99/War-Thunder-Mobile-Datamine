let skinViewPresets = {}
foreach(tag, list in skinsByTag)
  foreach(id in list) {
    if (id in skinViewPresets) {
      logerr($"Skin preset {id} added twice to skinsByTag")
      continue
    }
    skinViewPresets[id] <- { id, tag, image = $"{id}.avif" }
  }

return freeze(skinViewPresets)
